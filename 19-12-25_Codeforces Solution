Problem Link:
https://codeforces.com/problemset/problem/1742/C

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    const int n = 8;
    long t;
    cin >> t;

    while(t--) {
        vector<string> v(n);
        for(int i = 0; i < n; i++) 
        {
            cin >> v[i];
        }

        char s = '.';

        for(int r = 0; s == '.' && r < n; r++) 
        {
            bool f = true;
            for(int c = 0; c < n; c++) 
            {
                if(v[r][c] != 'R') 
                {
                    f = false;
                    break;
                }
            }
            if(f) 
                s = 'R';
        }

        for(int c = 0; s == '.' && c < n; c++) 
        {
            bool f = true;
            for(int r = 0; r < n; r++) 
            {
                if(v[r][c] != 'B') 
                {
                    f = false;
                    break;
                }
            }
            if(f) 
                s = 'B';
        }

        cout << s << "\n";
    }

    return 0;
}
