Problem Link:
https://codeforces.com/problemset/problem/1800/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    const string cs = "meow";

    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
        string s;
        cin >> s;

        int idx = 0;
        bool f = (s[0] == 'm' || s[0] == 'M');

        for(int i = 0; f && i < (int)s.size(); i++) 
        {
            if('A' <= s[i] && s[i] <= 'Z')
                s[i] += ('a' - 'A');

            if(s[i] == cs[idx]) 
            {
                continue;
            } 
            else if(idx + 1 < (int)cs.size() && s[i] == cs[idx + 1]) 
            {
                ++idx;
            } 
            else 
            {
                f = false;
            }
        }

        f = f && (idx == 3);
        cout << (f ? "YES\n" : "NO\n");
    }
    return 0;
}
