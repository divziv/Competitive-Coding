Problem Link:
https://codeforces.com/problemset/problem/1996/B

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t; 
    cin >> t;
    
    while(t--) 
    {
        int n, k;
        cin >> n >> k;
        
        vector<string> v(n);
        
        for(int i = 0; i < n; ++i) 
        {
            cin >> v[i];
        }

        int s = n / k;

        for(int i = 0; i < n; i += k) 
        {
            for(int j = 0; j < n; j += k) 
            {
                cout << v[i][j];
            }
            cout << "\n";
        }
    }

    return 0;
}
