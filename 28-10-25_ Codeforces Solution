Problem Link:
https://codeforces.com/problemset/problem/1141/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    long long n, m;
    cin >> n >> m;

    if(m % n != 0) 
    {
        cout << -1;
        return 0;
    }

    long long d = m / n;
    int c = 0;

    while(d % 2 == 0) 
    {
        d /= 2;
        c++;
    }
    while(d % 3 == 0) 
    {
        d /= 3;
        c++;
    }

    if(d != 1)
        cout << -1;
    else
        cout << c;

    return 0;
}
