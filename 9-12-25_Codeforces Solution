Problem Link:
https://codeforces.com/problemset/problem/1993/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    long t; 
    cin >> t;

    while(t--) {
        long n; 
        cin >> n;

        string s; 
        cin >> s;

        long a = 0, b = 0, c = 0, d = 0;
        for(char ch : s) 
        {
            a += (ch == 'A');
            b += (ch == 'B');
            c += (ch == 'C');
            d += (ch == 'D');
        }

        long cnt = min(a, n) + min(b, n) + min(c, n) + min(d, n);

        cout << cnt << "\n";
    }
}
