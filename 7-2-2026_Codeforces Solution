Problem Link:
https://codeforces.com/problemset/problem/1914/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    const int b = 26;
    
    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
        string s;
        cin >> s;

        int v[b] = {0};
        for(int i = 0; i < (int)s.size(); i++) 
        {
            ++v[s[i] - 'A'];
        }

        int c = 0;
        for(int i = 0; i < b; i++) 
        {
            c += (v[i] > i);
        }

        cout << c << "\n";
    }
}
