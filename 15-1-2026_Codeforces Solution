Problem Link:
https://codeforces.com/problemset/problem/1862/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    string s = "vika";
    int t; 
    cin >> t;
    
    while(t--) {
        int n, m; 
        cin >> n >> m;
        
        vector<string> v(n);
        for(int i = 0; i < n; i++) 
            cin >> v[i];

        int idx = 0;
        for(int c = 0; c < m; c++) 
        {
            for(int r = 0; r < n; r++) 
            {
                if(v[r][c] == s[idx]) 
                {
                    idx++;
                    break;
                }
            }
        }

        cout << (idx >= (int)s.size() ? "YES\n" : "NO\n");
    }
}
