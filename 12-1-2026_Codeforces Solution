Problem Link:
https://codeforces.com/problemset/problem/1794/B

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    while(t--) {
        int n;
        cin >> n;

        vector<long> a(n);
        for(int i = 0; i < n; i++) 
        {
            cin >> a[i];
            if(a[i] == 1) 
            {
                a[i] = 2;
            }
            if(i && a[i] % a[i - 1] == 0) 
            {
                ++a[i];
            }
        }

        for(int i = 0; i < n; i++) 
        {
            cout << a[i] << " ";
        }
        cout << "\n";
    }

    return 0;
}
