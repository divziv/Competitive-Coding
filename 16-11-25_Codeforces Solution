Problem Link:
https://codeforces.com/problemset/problem/766/B

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;
    vector<long long> v(n);
    for (auto &x : v) cin >> x;

    sort(v.begin(), v.end());

    for (int i = 0; i + 2 < n; i++) {
        if (v[i] + v[i + 1] > v[i + 2]) {
            cout << "YES";
            return 0;
        }
    }

    cout << "NO";
    return 0;
}
