Problem Link:
https://codeforces.com/problemset/problem/1926/B

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
        vector<string> v(n);
        for(int i = 0; i < n; ++i) 
            cin >> v[i];
        
        int top = n, bottom = -1, l = n, r = -1;
        
        for(int i = 0; i < n; ++i) 
        {
            for(int j = 0; j < n; ++j) 
            {
                if(v[i][j] == '1') 
                {
                    top = min(top, i);
                    bottom = max(bottom, i);
                    l = min(l, j);
                    r = max(r, j);
                }
            }
        }
        
        bool f = true;
        for(int i = top; i <= bottom; ++i) 
        {
            for(int j = l; j <= r; ++j) 
            {
                if(v[i][j] != '1') 
                {
                    f = false;
                    break;
                }
            }
            if(!f) 
                break;
        }
        
        if(f) 
            cout << "SQUARE\n";
        else 
            cout << "TRIANGLE\n";
    }
    return 0;
}
