Problem Link:
https://codeforces.com/problemset/problem/2167/C

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    while(t--) {
        int n;
        cin >> n;

        vector<int> a(n);
        bool e = false, o = false;

        for(int i = 0; i < n; i++) 
        {
            cin >> a[i];
            if(a[i] % 2 == 0) 
                e = true;
            else 
                o = true;
        }

        if(e && o) 
        {
            sort(a.begin(), a.end());
        }

        for(int i = 0; i < n; i++) 
        {
            cout << a[i] << " ";
        }
        cout << "\n";
    }

    return 0;
}
