Problem Link:
https://codeforces.com/problemset/problem/1783/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    long t;
    cin >> t;
    
    while(t--) {
        long n;
        cin >> n;
        vector<long> p(n);
        for(long i = 0; i < n; i++) 
            cin >> p[i];

        sort(p.rbegin(), p.rend());

        if(p[0] == p.back()) 
        {
            cout << "NO\n";
        } 
        else 
        {
            cout << "YES\n";
            cout << p.back() << " ";
            for(long i = 0; i < n - 1; i++) 
            {
                cout << p[i] << " ";
            }
            cout << "\n";
        }
    }

    return 0;
}
