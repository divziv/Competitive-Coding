Problem Link:
https://codeforces.com/problemset/problem/1473/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;
    
    while(t--) {
        int n, d;
        cin >> n >> d;
        
        vector<int> v(n);
        for(int &x : v) 
            cin >> x;

        int m = *max_element(v.begin(), v.end());
        if(m <= d) 
        {
            cout << "YES\n";
            continue;
        }

        sort(v.begin(), v.end());
        if(v[0] + v[1] <= d) 
            cout << "YES\n";
        else 
            cout << "NO\n";
    }
    return 0;
}
