Problem Link:
https://codeforces.com/problemset/problem/1692/F

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {  
    int t;
    cin >> t;
    
    while(t--) {
        int n;
        cin >> n;
        
        vector<int> v(n);
        for(int i = 0; i < n; ++i) 
            cin >> v[i];
        
        vector<int> c[10];
        for(int x : v) 
        {
            int y = x % 10;
            if((int)c[y].size() < 3)
                c[y].push_back(x);
        }
        
        vector<int> d;
        for(int i = 0; i < 10; ++i)
            for(int x : c[i]) 
                d.push_back(i);
        
        bool f = false;
        int m = d.size();
        for(int i = 0; i < m && !f; ++i)
            for(int j = i + 1; j < m && !f; ++j)
                for(int k = j + 1; k < m && !f; ++k)
                    if((d[i] + d[j] + d[k]) % 10 == 3)
                        f = true;
        
        cout << (f ? "YES\n" : "NO\n");
    }
}
