Problem Link:
https://codeforces.com/problemset/problem/1881/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    long t;
    cin >> t;
    
    while(t--) {
        long n, m;
        cin >> n >> m;

        string x, s;
        cin >> x >> s;

        int c = -1;

        for(int p = 0; p <= 6; p++) 
        {
            if(x.find(s) != string::npos) 
            {
                c = p;
                break;
            }
            x = x + x;
        }

        cout << c << "\n";
    }

    return 0;
}
