Problem Link:
https://codeforces.com/problemset/problem/1857/C

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    long t;
    cin >> t;

    while(t--) {
        long n;
        cin >> n;

        long m = n * (n - 1) / 2;
        vector<long> v(m);

        for(long p = 0; p < m; p++) 
        {
            cin >> v[p];
        }

        sort(v.begin(), v.end());

        long i = 0;
        long s = n - 1;
        vector<long> g;

        while(i < m) 
        {
            g.push_back(v[i]);
            i += s;
            --s;
        }

        g.push_back(g.back());

        for(long p = 0; p < n; p++) 
        {
            cout << g[p] << " ";
        }
        cout << "\n";
    }

    return 0;
}
