Problem Link:
https://codeforces.com/problemset/problem/1097/B

Solution:
#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n;
    cin >> n;
    
    vector<int> v(n);
    
    for(int i = 0; i < n; ++i) 
        cin >> v[i];
    int t = 1 << n;
    
    for(int m = 0; m < t; ++m) 
    {
        int s = 0;
        for(int i = 0; i < n; ++i) 
        {
            if(m & (1 << i)) 
                s += v[i];
            else 
                s -= v[i];
        }
        
        if((s % 360 + 360) % 360 == 0) 
        {
            cout << "YES\n";
            return 0;
        }
    }
    cout << "NO\n";
    return 0;
}
