Problem Link:
https://codeforces.com/contest/2162/problem/A

Solution:
#include <bits/stdc++.h>
using namespace std;

int main() {
    int t;
    cin >> t;

    while(t--) {
        int n;
        cin >> n;

        vector<int> v(n);
        for(int i = 0; i < n; ++i) 
        {
            cin >> v[i];
        }

        int m = 0;

        for(int i = 0; i < n; ++i) 
        {
            int s = 0;
            for(int j = i; j < n; ++j) 
            {
                s += v[j];
                int l = j - i + 1;
                int a = s / l;
                if(a > m) 
                {
                    m = a;
                }
            }
        }

        cout << m << "\n";
    }

    return 0;
}
